<a
  href="{% if section.settings.logo %}{{ section.settings.logo }}{% else %}{{ routes.root_url }}{% endif %}"
  aria-current="page"
  class="brand w-inline-block w--current"
>
  {%- if section.settings.lottie_animation_3_visibility -%}
